{
  "index": "/index.html",
  "assetGroups": [{
    "name": "app",
    "installMode": "prefetch",
    "resources": {
      "files": [
        "/favicon.ico",
        "/index.html"
      ],
      "versionedFiles": [
        "/*.bundle.css",
        "/*.bundle.js",
        "/*.chunk.js"
      ]
    }
  }, {
    "name": "assets",
    "installMode": "lazy",
    "updateMode": "prefetch",
    "resources": {
      "files": [
        "/assets/**"
      ]
    }
  }],
  "dataGroups": [{
    "name": "user",
    "urls": [
      "http://localhost:3000/user"
    ],
    "cacheConfig": {
      "maxSize": 100,
      "maxAge": "3d",
      "timeout": "1m",
      "strategy": "freshness"
    }
  }],
  "push": {
    "showNotifications" : true,
    "backgroundOnly" : false
    }
}
